{
  "name": "todo-list",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "lodash": "^4.17.15",
    "mysql2": "^3.11.5",
    "nodemon": "^3.1.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.1.1",
    "react-scripts": "3.4.0",
    "react-uuid": "^1.0.2"
  },
  "scripts": {
    "start": "concurrently --names \"npm run config\" \"npm run db-server\"  \"npm run update-env \" \"set PORT=3003 && react-scripts start\" ",
    "db-server": "json-server --watch src/data/database/db.json --port 3001",
    "express-server": "nodemon --inspect express-server.js",
    "update-env": "nodemon --watch .env --exec 'npm start'",
    "config": "node config.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^9.1.0"
  }
}
